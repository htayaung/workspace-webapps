@using MudBlazor;
<MudBlazor.MudText Typo="Typo.h6" Align="Align.Center">@Title</MudBlazor.MudText>
<MudBlazor.MudPaper>
    <MudBlazor.MudChart ChartType="ChartType.Bar"
                        ChartSeries="@Series"
                        XAxisLabels="@XAxisLabels"
                        ChartOptions="options">
    </MudBlazor.MudChart>
</MudBlazor.MudPaper>


@code {
    [Parameter]
    public List<ChartSeries> Series { get; set; }

    [Parameter]
    public string[] XAxisLabels { get; set; }

    [Parameter]
    public string Title { get; set; }

    private ChartOptions options = new();

    protected override void OnInitialized()
    {
        base.OnInitialized();
    }
}
